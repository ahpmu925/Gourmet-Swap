ALTER proc [dbo].[Blogs_DeleteById]
	@Id int


	/*
SELECT *
FROM dbo.Blogs
WHERE Id = 3

Execute dbo.Blogs_DeleteById
	@Id = 3

SELECT *
FROM dbo.Blogs
WHERE Id = 3

	*/

	as

	BEGIN

DELETE FROM [dbo].[Blogs]
      WHERE Id = @Id

END


