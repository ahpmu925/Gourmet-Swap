ALTER proc [dbo].[Messages_SelectAll]


/*

Execute dbo.Messages_SelectAll

	*/

	as

	BEGIN

SELECT [Id]
      ,[FirstName]
      ,[LastName]
      ,[Email]
      ,[PhoneNo]
      ,[City]
      ,[Address]
      ,[Message]
	  ,[UserId]
      ,[DateAdded]
      ,[DateModified]
	  ,[Status]
  FROM [dbo].[Messages]


END
